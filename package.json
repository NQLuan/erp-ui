{
  "name": "company-ui",
  "version": "1.0.0",
  "description": "My impressive Nuxt.js project",
  "author": "hoangnguyen",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "dev": "cross-env NODE_ENV=development pm2 start nuxt server/index.js --name 'company-ui' --watch server",
    "dev-debug": "node --inspect node_modules/.bin/nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production pm2 start nuxt server/index.js --name 'company-ui'",
    "stop": "pm2 stop 'company-ui'",
    "restart": "pm2 restart 'company-ui'",
    "delete": "pm2 delete 'company-ui'",
    "kill": "pm2 kill",
    "list": "pm2 ls",
    "generate": "nuxt generate"
  },
